{
  "name": "react-native-worker-queue",
  "title": "React Native Worker Queue",
  "version": "0.4.2",
  "description": "A worker/job queue for React Native",
  "react-native": "src/index.ts",
  "types": "lib/typescript/src",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "scripts": {
    "start": "react-native start",
    "validate:eslint": "tslint \"src/**/*.{js,ts,tsx}\" \"example/**/*.{js,ts,tsx}\"",
    "validate:typescript": "tsc --project ./ --noEmit",
    "test": "jest --coverage",
    "prepare": "bob build",
    "typedoc": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanTigranyan/react-native-worker-queue.git",
    "baseUrl": "https://github.com/VanTigranyan/react-native-worker-queue"
  },
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Van",
    "email": "tigranyan.van@gmail.com"
  },
  "license": "MIT",
  "licenseFilename": "LICENSE",
  "readmeFilename": "README.md",
  "peerDependencies": {
    "react": "^16.5.0",
    "react-native": ">=0.57.0-rc.0 <1.0.x"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.17.1",
    "@types/jest": "^24.0.17",
    "@types/react": "^17.0.1",
    "@types/react-native": "0.64.1",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react": "^17.0.1",
    "react-native": "^0.64.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "tslint-react-native": "^0.0.7",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.8.2",
    "typescript": "^4.2.4"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
